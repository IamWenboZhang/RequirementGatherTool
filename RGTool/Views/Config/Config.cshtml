@model RGTool.Models.Config

@{
    ViewBag.Title = "Config";
}
<head>
    <meta charset="UTF-8" />
    <title></title>
    <script src="~/Scripts/jquery-3.3.1.js"></script>
    <script src="https://code.angularjs.org/1.3.9/angular.js"></script>
    <link href="~/Content/Office.css" rel="stylesheet" type="text/css" />
    <script src="https://appsforoffice.microsoft.com/lib/1.1/hosted/office.js" type="text/javascript"></script>

    <link rel="stylesheet" href="https://agave.azurewebsites.net/Fabric/css/fabric.min.css">
    <link rel="stylesheet" href="https://agave.azurewebsites.net/Fabric/css/fabric.components.css">
    <script src="https://agave.azurewebsites.net/Fabric/components/NavBar/Jquery.NavBar.js"></script>

    <script src="~/Scripts/Office/ExcelHelper.js"></script>
    <script src="~/Scripts/Office/WordHelper.js"></script>
    <script src="~/Scripts/Section/SectionHelper.js"></script>
    <script src="~/Scripts/Config/ConfigHelper.js"></script>
    <script src="~/Scripts/Common/GatherHelper.js"></script>

</head>
<div id="tbPanel">
    <div ng-controller="ConfigController" id="team">
        <h3>Config</h3>
        @*<p>Total Hours: <input type="text" ng-model="hours" /></p>*@
        TD ShortName:<input id="ip_shortname" type="text" /><br /><br />
        TD Version:<input id="ip_version" type="text" /><br /><br />
        TDType:<select id="select_tdtype">
            <option value="None">None</option>
            <option value="Block">Block</option>
            <option value="FileFormat">FileFormat</option>
            <option value="SOAP">SOAP</option>
            <option value="WOPI">WOPI</option>
        </select><br /><br />
        StartSection:<input id="ip_startsection" type="text" /><br /><br />
        EndSection:<input id="ip_endsection" type="text" /><br /><br />
        @*Lead: <select ng-model="lead" ng-options="opt as opt.name for opt in listItems"></select> <br />
            UI Designer: <select ng-model="ui" ng-options="opt as opt.name for opt in listItems"></select><br />
            Web Engineer: <select ng-model="eng1" ng-options="opt as opt.name for opt in listItems"></select><br />
            DB Engineer: <select ng-model="eng2" ng-options="opt as opt.name for opt in listItems"></select><br />*@
    </div>
    <button id="set-data" onclick="setconfig()">Set Config</button><br />
</div>




